<% objects.each do |object| %>
  <% if local_assigns.has_key?(:renderer_id) && local_assigns.has_key?(:renderer_type) %>
    <%= render 'shared/card', card_type: card_type, object: object, renderer_id: renderer_id, renderer_type: renderer_type %>
  <% else %>
    <%= render 'shared/card', card_type: card_type, object: object %>
  <% end %>
<% end %>
