<% notes.each do |note| %>
  <% if local_assigns.has_key?(:referrer_id) && local_assigns.has_key?(:referrer_type) %>
    <%= render 'shared/card', card_type: card_type, object: note, referrer_id: referrer_id, referrer_type: referrer_type, show_unlink: show_unlink %>
  <% else %>
    <%= render 'shared/card', card_type: card_type, object: note, show_unlink: false %>
  <% end %>
<% end %>
