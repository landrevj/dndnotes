<table>
  <strong>Quests</strong>
  <thead>
    <th>Quest</th>
    <th>ID</th>
    <th colspan="1"></th>
  </thead>

  <tbody>
    <% quests.each do |quest| %>
      <tr>
        <td><%= quest.name %></td>
        <td><%= quest.id %></td>
        <td><%= link_to 'Unlink', Link.find_link([id, type],[quest.id, 'Quest']).first, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
